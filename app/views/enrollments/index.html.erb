<%if current_user.has_role?(:user)%>

<% @enrollment_to.each do |enrollment|%>
<%= enrollment.from_user.email %>
<%= link_to "個人チャット", enrollment_path(enrollment) %>
<br />
<% end %>


<% elsif  current_user.has_role?(:recruiter) %>

<% @enrollment_from.each do |enrollment|%>

<%= enrollment.to_user.email %>
<%= link_to "個人チャット", enrollment_path(enrollment) %>
<br />


<% end %>
<% end %>